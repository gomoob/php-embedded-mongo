{
	"name" : "gomoob/php-embedded-mongo",
	"description" : "Start an embedded Mongo DB server and run your PHPUnit integration tests easily.",
	"version" : "0.0.1-alpha2",
	"license" : "MIT",
	"type" : "library",
	"keywords" : [
		"gomoob",
		"mongo",
		"embedded",
		"in memory",
		"phpunit"
	],
	"authors" : [{
			"name" : "GoMoob",
			"email" : "contact@gomoob.com",
			"homepage" : "http://www.gomoob.com"
		}, {
			"name" : "Baptiste Gaillard",
			"email" : "baptiste.gaillard@gomoob.com"
		}, {
			"name" : "Simon Baudry",
			"email" : "simon.baudry@gomoob.com"
		}
	],
	"autoload" : {
		"psr-4" : {
			"Gomoob\\" : "src/main/php/Gomoob"
		}
	},
	"autoload-dev" : {
		"psr-4" : {
			"Gomoob\\" : "src/test/php/Gomoob"
		}
	},
	"require" : {
		"php" : ">=5.3.3",
		"monolog/monolog" : "~1.17",
		"psr/log" : "~1.0"
	},
	"require-dev" : {
		"codeclimate/php-test-reporter" : "~0.2",
		"pdepend/pdepend" : "~2.2",
		"phploc/phploc" : "~2.1",
		"phpmd/phpmd" : "~2.3",
		"phpunit/phpunit" : "~4.8",
		"satooshi/php-coveralls" : "~0.6",
		"sebastian/phpcpd" : "~2.0",
		"squizlabs/php_codesniffer" : "~2.3"
	}
}